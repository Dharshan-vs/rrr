{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "devCommand": "npm run dev",
  "github": {
    "enabled": true,
    "silent": true
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": true,
      "staging": true,
      "feature/*": true,
      "bugfix/*": true,
      "hotfix/*": true
    }
  }
} 